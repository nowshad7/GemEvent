@{
	Layout = null;
}
<!doctype html>
<html lang="en">
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
	<meta name="description" content="">
	<meta name="author" content="">
	<link rel="icon" href="~/Content/asset/gem.png">

	<title>Gem Events</title>
	<!-- Bootstrap core CSS -->
	<link href="~/Content/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">
	<!--fontawesome-->
	<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.0.13/css/all.css" integrity="sha384-DNOHZ68U8hZfKXOrtjWvjxusGo9WQnrNx2sqG0tfsghAvtVlRW3tvkXWZh58N9jp" crossorigin="anonymous">
	<!-- Custom styles for this template -->
	<link rel="stylesheet" href="~/Content/CSS/service.css">
	<!--font-family : poppins-->
	<link href="https://fonts.googleapis.com/css?family=Poppins:200,300,400,700,900" rel="stylesheet">
	<!--font-family : Tangerine-->
	<link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Tangerine">
	<!--font-family : Dancing Script-->
	<link href="https://fonts.googleapis.com/css?family=Cormorant+Garamond" rel="stylesheet">
</head>
<body>

	<header>
		<nav class="navbar navbar-expand-md navbar-dark fixed-top " style="background-color: rgba(0,0,0,0.9);">
			<div class="container">
				@Html.ActionLink("Gem Events", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
				<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation">
					<span class="navbar-toggler-icon"></span>
				</button>
				<div class="collapse navbar-collapse" id="navbarCollapse">
					<ul class="navbar-nav ml-auto">
						<li class="nav-item ">
							@Html.ActionLink("Home", "Index", "Home")
						</li>
						<li class="nav-item active">
							@Html.ActionLink("Service", "Service", "Home")
						</li>
						<li class="nav-item">
							@Html.ActionLink("About", "About", "Home")
						</li>
						<li class="nav-item">
							@Html.ActionLink("Gellary", "Index", "igms")
						</li>
						<li class="nav-item">
							@Html.ActionLink("Contact", "Contact", "Home")
						</li>

					</ul>
				</div>
			</div>
		</nav>
	</header>
	<main role="main">


		<div class="up">
			<div class="child">
				<div class="title">
					<h1>Gem Events at your service</h1>
					<h4>Turn your dream into a reality</h4>
				</div>
			</div>
		</div>
		<section class="jumbotron text-center">
			<div class="container">
				<h1 class="jumbotron-heading">WEDDINGS</h1>

			</div>
		</section>
		<!--form-->

		<div class="album py-5 bg-light">
			<div class="container">

				<div class="row">
					<div class="col-lg-4">
						<img class="rounded-circle" src="~/Content/asset/decoration.jpg" alt="Generic placeholder image" width="200" height="200">
						<h2>Full Package</h2>
						<p>Here we give your full weddimg package Pre Reception time, Reception & Ceremony ,The Beginning stages and finally On the Day of the Wedding.</p>
					</div><!-- /.col-lg-4 -->
					<div class="col-lg-4">
						<img class="rounded-circle" src="~/Content/asset/theme2.jpg" alt="Generic placeholder image" width="200" height="200">
						<h2>Partial Package</h2>
						<p>Here you can chose any of our Wedding packages as you want.</p>
					</div><!-- /.col-lg-4 -->
					<div class="col-lg-4">
						<img class="rounded-circle" src="~/Content/asset/theme6.jpg" alt="Generic placeholder image" width="200" height="200">
						<h2>On the Day</h2>
						<p>Check Bridal party preparations – ensure all suppliers such as make-up artists, hairdressers and all other items arranged have arrived.</p>
					</div><!-- /.col-lg-4 -->
				</div><!-- /.row -->
			</div>
		</div>

		<!--end form-->

		<section class="jumbotron text-center">
			<div class="container">
				<h1 class="jumbotron-heading">PRIVATE PARTIES</h1>

			</div>
		</section>
		<!--form-->

		<div class="album py-5 bg-light">
			<div class="container">

				<div class="row">
					<div class="col-lg-4">
						<img class="rounded-circle" src="~/Content/asset/bd.jpg" alt="Generic placeholder image" width="200" height="200">
						<h2>Birthday Party</h2>
						<p>Here our approach to create a beautiful party to celebrate your birthday.</p>
					</div><!-- /.col-lg-4 -->
					<div class="col-lg-4">
						<img class="rounded-circle" src="~/Content/asset/re.jpg" alt="Generic placeholder image" width="200" height="200">
						<h2>Reunion Party</h2>
						<p>Here we organize such a reunion party to make you old friend more close to you. </p>
					</div><!-- /.col-lg-4 -->
					<div class="col-lg-4">
						<img class="rounded-circle" src="~/Content/asset/pri.jpg" alt="Generic placeholder image" width="200" height="200">
						<h2>Private Events</h2>
						<p>Here we organize all kind of private party to make your moment beautiful and memorable.</p>
					</div><!-- /.col-lg-4 -->
				</div><!-- /.row -->
			</div>
		</div>

		<section class="jumbotron text-center">
			<div class="container">
				<h1 class="jumbotron-heading">CORPORATE EVENTS</h1>

			</div>
		</section>
		<!--form-->

		<div class="album py-5 bg-light">
			<div class="container">

				<div class="row">
					<div class="col-lg-4">
						<img class="rounded-circle" src="~/Content/asset/c3.jpg" alt="Generic placeholder image" width="200" height="200">
						<h2>Conferences</h2>
						<p>Here our approach to create a beautiful party to celebrate your birthday.</p>
					</div><!-- /.col-lg-4 -->
					<div class="col-lg-4">
						<img class="rounded-circle" src="~/Content/asset/c2.jpg" alt="Generic placeholder image" width="200" height="200">
						<h2>Corporate Parties</h2>
						<p>Here we organize such a reunion party to make you old friend more close to you. </p>
					</div><!-- /.col-lg-4 -->
					<div class="col-lg-4">
						<img class="rounded-circle" src="~/Content/asset/c1.jpg" alt="Generic placeholder image" width="200" height="200">
						<h2>Employee Fun Days</h2>
						<p>Here we organize all kind of private party to make your moment beautiful and memorable.</p>
					</div><!-- /.col-lg-4 -->
				</div><!-- /.row -->
			</div>
		</div>


		<section class="jumbotron text-center">
			<div class="container">
				<button class="btn">@Html.ActionLink("BOOK A CONSULTATION", "Login", "Account")</button>

			</div>
		</section>




		<!-- FOOTER -->
		<div class="container">
			<footer>
				<p class="follow">
					Follow Us
					<i class="fab fa-facebook-f"></i>
					<i class="fab fa-instagram"></i>
					<i class="fab fa-twitter"></i>
					<i class="fab fa-pinterest"></i>
				</p>
				<p>&copy;  COPYRIGHT 2018. ALL RIGHTS RESERVED,GEM EVENTS.</p>

			</footer>
		</div>
	</main>

	<!-- Bootstrap core JavaScript
	================================================== -->

	<script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
	<script>window.jQuery || document.write('<script src="~/Content/bootstrap/assets/js/vendor/jquery-slim.min.js"><\/script>')</script>
	<script src="~/Content/bootstrap/assets/js/vendor/popper.min.js"></script>
	<script src="~/Content/bootstrap/dist/js/bootstrap.min.js"></script>
	<script src="~/Content/bootstrap/assets/js/vendor/holder.min.js"></script>
</body>
</html>